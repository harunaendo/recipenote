<div class='main-container'>
	<div class='row'>
	  <div class='col-9 mt-4 ml-4 mb-4'>
            <% if @model == 'user' %>
                <h3>ユーザー検索結果"<%= @content %>"</h3>
              <%= render 'user/users/index', users: @records %>
            <% elsif @model == 'recipe' %>
                <h3>レシピ検索結果 "<%= @content %>"</h3>
              <%= render 'user/recipes/index', recipes: @records %>
            <% end %>
      </div>
    </div>
</div>
