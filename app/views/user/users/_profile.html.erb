 <% if user.profile_image.attached? %>
    <%= image_tag user.get_profile_image(100,100) %>
 <% else %>
    <%= image_tag 'no_image', size: "100x100" %>
 <% end %>
<div>
  <h3><%=  user.name %></h3>
  <% @recipes.each do |recipe| %>
      <% if recipe.user == current_user %>
        <%= link_to "編集",edit_user_user_path(user.id) %>
      <% else %>
        <td></td>
      <% end %>
  <% end %>
</div>