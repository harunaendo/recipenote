  　　 <% users.each do |user| %>
          <p></p>
          <% if user.profile_image.attached? %>
            <%= image_tag user.profile_image, size: "200x200" %>
          <% else %>
            <%= image_tag 'no_image.jpg', size: "200x200" %>
          <% end %>
          <%= link_to user.name, user_user_path(user.id) %>
    　<% end %>

